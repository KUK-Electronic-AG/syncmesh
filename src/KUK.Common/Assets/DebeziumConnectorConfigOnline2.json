{
  "topic.prefix": "new_to_old",
  "name": "new-to-old-connector{connectorNameSuffix}",
  "transforms": "unwrap,route",
  "database.hostname": "{newDatabaseHostName}",
  "connector.class": "io.debezium.connector.postgresql.PostgresConnector",
  "database.port": "{newDatabasePort}",
  "tasks.max": "1",
  "database.user": "{newDatabaseUserName}",
  "key.converter": "org.apache.kafka.connect.json.JsonConverter",
  "database.password": "{newDatabasePassword}",
  "value.converter": "org.apache.kafka.connect.json.JsonConverter",
