{
    "topic.prefix": "old_to_new",
    "name": "old-to-new-connector{connectorNameSuffix}",
    "transforms": "unwrap,route",
    "database.hostname": "{oldDatabaseHostName}",
    "connector.class": "io.debezium.connector.mysql.MySqlConnector",
    "database.port": "{oldDatabasePort}",
    "tasks.max": "1",
    "database.user": "{oldDatabaseUserName}",
    "key.converter": "org.apache.kafka.connect.json.JsonConverter",
    "database.password": "{oldDatabasePassword}",
    "value.converter": "org.apache.kafka.connect.json.JsonConverter",
    "database.server.id": "3739328007",
    "table.include.list": "{tableIncludeList}",
    "include.schema.changes": "true",
    "include.query": "true",
    "database.dbname": "{dbName}",
    "database.server.name": "{dbName}_kafka",
    "database.sslmode": "REQUIRED",
    "delete.tombstone.handling.mode": "tombstone",
    "key.converter.basic.auth.credentials.source": "USER_INFO",
    "key.converter.schema.registry.basic.auth.user.info": "{schemaRegistryLoginAndPassword}",
    "key.converter.schema.registry.url": "{schemaRegistryUrlAndPort}",
    "key.converter.schemas.enable": "false",
    "schema.history.internal.consumer.security.protocol": "SSL",
    "schema.history.internal.consumer.ssl.key.password": "{sslKeyPassword}",
    "schema.history.internal.consumer.ssl.keystore.location": "{keystoreLocation}",
    "schema.history.internal.consumer.ssl.keystore.password": "{keystorePassword}",
    "schema.history.internal.consumer.ssl.keystore.type": "PKCS12",
    "schema.history.internal.consumer.ssl.truststore.location": "{truststoreLocation}",
    "schema.history.internal.consumer.ssl.truststore.password": "{truststorePassword}",
    "schema.history.internal.kafka.bootstrap.servers": "{bootstrapServerAndPort}",
    "schema.history.internal.kafka.topic": "schema-changes.{dbName}",
    "schema.history.internal.producer.security.protocol": "SSL",
    "schema.history.internal.producer.ssl.key.password": "{sslKeyPassword}",
    "schema.history.internal.producer.ssl.keystore.location": "{keystoreLocation}",
    "schema.history.internal.producer.ssl.keystore.password": "{keystorePassword}",
    "schema.history.internal.producer.ssl.keystore.type": "PKCS12",
    "schema.history.internal.producer.ssl.truststore.location": "{truststoreLocation}",
    "schema.history.internal.producer.ssl.truststore.password": "{truststorePassword}",
    "value.converter.basic.auth.credentials.source": "USER_INFO",
    "value.converter.schema.registry.basic.auth.user.info": "{schemaRegistryLoginAndPassword}",
    "value.converter.schema.registry.url": "{schemaRegistryUrlAndPort}",
    "value.converter.schemas.enable": "false",
    "transforms.route.regex": ".*",
    "transforms.route.replacement": "old_to_new.{dbName}",
    "transforms.route.type": "org.apache.kafka.connect.transforms.RegexRouter",
    "transforms.unwrap.add.fields": "op,source.ts_ms,source.table,source.name,query",
    "transforms.unwrap.delete.handling.mode": "rewrite",
    "transforms.unwrap.type": "io.debezium.transforms.ExtractNewRecordState",
    "snapshot.mode": "schema_only",
    "database.history.kafka.topic": "dbhistory.{dbName}"
}